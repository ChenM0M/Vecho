{
  "identifier": "default",
  "description": "Default capabilities for Vecho Studio",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "core:window:default",
    "core:window:allow-close",
    "core:window:allow-minimize",
    "core:window:allow-maximize",
    "core:window:allow-unmaximize",
    "core:window:allow-start-dragging",

    "allow-get-data-root",
    "allow-load-state",
    "allow-save-state",
    "allow-import-url",
    "allow-upload-begin",
    "allow-upload-chunk",
    "allow-upload-finish",

    "allow-transcribe-media",
    "allow-optimize-transcription",
    "allow-summarize-media",
    "allow-chat-media",
    "allow-export-media"
  ]
}
